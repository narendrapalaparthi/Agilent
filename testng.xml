<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="AgilentAutomation">
	<listeners>
		<listener class-name="com.agilent.cps.utils.Listener"></listener>
	</listeners>
	<test name="CPS Tests">
		<groups>
			<run>
				<include name="author"></include>
				<include name="test"></include>
			</run>
		</groups>
		<classes>
			<class name="com.agilent.cps.tests.tests" />
			<class name="com.agilent.cps.author.AuthorTests" />
		</classes>
	</test>

</suite>